{
  "name": "react-mounter",
  "version": "1.2.0",
  "description": "React Mounter lets you mount React components to DOM easily.",
  "repository": {
    "type": "git",
    "url": "https://github.com/kadirahq/react-mounter.git"
  },
  "license": "MIT",
  "options": {
    "mocha": "--require scripts/mocha_runner lib/**/__tests__/**/*.js"
  },
  "scripts": {
    "prepublish": ". ./scripts/prepublish.sh",
    "lint": "eslint ./lib",
    "lintfix": "eslint ./lib --fix",
    "testonly": "mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "chai": "3.x.x",
    "eslint": "^6.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "3.x.x",
    "mocha": "^6.1.4",
    "mock-require": "1.x.x",
    "nodemon": "^1.19.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sinon": "1.17.x"
  },
  "peerDependencies": {
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "domready": "1.x.x"
  }
}
